<!DOCTYPE html>
<html>
  <head>
    <title>Api</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <% if current_user %>
      <%= link_to "Consumers", admin_consumers_path %> | <%= link_to "Services", admin_services_path %> | <%=  link_to "Users", admin_users_path %>
    <% end %>

    <% if notice %>
      <div style="width: 80%; margin: 20px;" class="alert alert-success"><%= notice %></div>
    <% end %>
    <% if alert %>
      <div style="width: 80%; margin: 20px;" class="alert alert-block"><%= alert %></div>
    <% end %>

    <div class="row" style="margin-top: 15px;">
      <div class="span12 offset2">
        <%= yield %>
      </div>
    </div>

  </body>
</html>
